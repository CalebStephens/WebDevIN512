<nav> 
    <div class = "apiSkip" title="Skip to API">API</div>  
    <div class = "logo">
        <img src = "img/newLogo.png" width = "110" height = "100">
    </div>
    <div class = "videoSkip" title="Skip to Video">VIDEO</div>
    
</nav>

<div class = "container full flexCenter">
    <div class="rotate"><h1>CARS</h1></div>
</div>


 <div class = "first-car full"></div>
 <div class = "second-car full"></div>

<div class = "trio-text full flexCenter">
    <div class = "trio1">WHAT WOULD</div>
    <div class = "trio2">YOUR DREAM</div>
    <div class = "trio3">CAR BE?</div>
</div>


<div class = "scroll-container">

    <div class="wrapper-api">
        <% for(let i=0;i<3;i++){ %>
            <div class="one flexCenter"><img src = "" class="testing"><div class = "artist">Hello</div></div>
        <% } %>
    </div>
    <div class="more-images flexCenter"><a href = "https://unsplash.com/s/photos/Car" target = "_blank" rel = "noopener">More Photos</a></div>

    <div class = "small-Grid">
        <% for(let i=0;i<6;i++){ %>
        <div class="first-grid-column">
            <% for(let i=0;i<5;i++){ %>
                <div class = "grid-section flexCenter"></div>
            <% } %>
        </div>
        <% } %>
    </div>

    <div class = "outer full flexCenter">
        <div class="video-container">
            <video muted autoplay loop>
                <source src="img/Pexels Videos 3700.mp4" type="video/mp4">
                Your browser does not support the video tag.
            </video>
        </div>
        <div class = "video-text">Late Night Drives</div>
    </div>

    <div class="grid-container">
        <div class="item flexCenter"><img src = "img/lambo-logo.jpeg" alt= "Lamborghini Badge"></div>
        <div class="item flexCenter"><a href = "https://www.lamborghini.com/en-en" target = "_blank" rel = "noopener"><h2>Lamborghini</h2></a></div>
        <div class="item flexCenter"><a href = "https://www.porsche.com/pap/_newzealand_/" target = "_blank" rel = "noopener"><h2>Porsche</h2></a></div>
        <div class="item flexCenter"><img src = "img/porshe-wheel.jpeg" alt="Porsche Wheel"></div>
        <div class="item flexCenter"><img src = "img/merc-logo.jpeg" alt="Mercedes-Benz Badge"></div>
        <div class="item flexCenter"><a href = "https://www.mercedes-benz.co.nz/passengercars.html?group=all&subgroup=see-all&view=BODYTYPE" target = "_blank" rel = "noopener"><h2>Mercedes-Benz</h2></a></div>

    </div>

    <div class = "buttonContainer flexCenter">
        <button class = "button">Back to Top</button>
    </div>

    <footer class="flexCenter">
        <p>Made By Caleb Stephens</p>
    </footer>
</div>


        
    

